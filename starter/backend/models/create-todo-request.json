{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "todo",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "The name of the todo",
      "minLength": 1
    },
    "dueDate": {
      "type": "string",
      "format": "date",
      "description": "The due date for the todo"
    }
  },
  "required": ["name", "dueDate"],
  "additionalProperties": false
}
